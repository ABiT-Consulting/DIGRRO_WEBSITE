    "version": "0.2.0",
    "configurations": [
        {
            "type": "node-terminal",
            "request": "launch",
            "name": "Vite: Dev Server",
            "command": "npm run dev",
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "pwa-chrome",
            "request": "launch",
            "name": "Vite: Chrome",
            "url": "http://localhost:5173",
            "webRoot": "${workspaceFolder}"
        }
    ],
    "compounds": [
        {
            "name": "Vite: Dev + Chrome",
            "configurations": [
                "Vite: Dev Server",
                "Vite: Chrome"
            ]
        }
    ]
}
